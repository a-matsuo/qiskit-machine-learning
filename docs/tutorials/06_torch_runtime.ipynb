            "outputs": [],
            "source": [
                "def score_func(out, target):\n",
                "    from numpy import sign\n",
                "\n",
                "    score = 0\n",
                "    if sign(out.item()) == target.item():\n",
                "        score = 1\n",
                "    return score\n",
